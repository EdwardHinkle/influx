{
  "name": "influx",
  "version": "0.1.0",
  "description": "A multi-user microsub server",
  "main": "server.js",
  "scripts": {
    "start": "node ./server.js --harmony_array_includes",
    "build": "tsc",
    "freshbuild": "npm run build; npm start",
    "startServer": "npm run build; forever start ./server.js",
    "restartServer": "npm run build; forever restart ./server.js",
    "updateServer": "git pull; npm run restartServer"
  },
  "repository": {
    "type": "git",
    "url": "git+https://edwardhinkle@github.com/EdwardHinkle/influx.git"
  },
