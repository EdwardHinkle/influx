  "bugs": {
    "url": "https://github.com/EdwardHinkle/influx/issues"
  },
  "homepage": "https://github.com/EdwardHinkle/influx#readme",
  "dependencies": {
    "@types/body-parser": "^1.16.5",
    "@types/cheerio": "^0.22.3",
    "@types/cron": "^1.2.1",
    "@types/express": "^4.0.37",
    "@types/express-session": "^1.15.3",
    "@types/moment": "^2.13.0",
    "@types/request": "^2.0.4",
    "body-parser": "^1.18.2",
    "cheerio": "^1.0.0-rc.2",
    "cron": "^1.3.0",
    "express": "^4.15.5",
    "express-session": "^1.15.6",
